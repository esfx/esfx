{
    "name": "@esfx/async-iter-hierarchy",
    "version": "1.0.0-pre.39",
    "description": "A Symbol-based API for defining an ECMAScript AsyncIterable with an inherent hierarchy.",
    "type": "commonjs",
    "types": "./dist/types/index.d.ts",
    "main": "./dist/cjs/index.js",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.mjs"
        },
        "./hierarchy": {
            "types": "./dist/types/hierarchy.d.ts",
            "require": "./dist/cjs/hierarchy.js",
            "import": "./dist/esm/hierarchy.mjs"
        },
        "./ordered": {
            "types": "./dist/types/ordered.d.ts",
            "require": "./dist/cjs/ordered.js",
            "import": "./dist/esm/ordered.mjs"
        }
    },
    "typesVersions": {
        "*": {
            "hierarchy": [
                "./dist/types/hierarchy.d.ts"
            ],
            "ordered": [
                "./dist/types/ordered.d.ts"
            ]
        }
    },
    "author": "Ron Buckton (rbuckton@chronicles.org)",
    "license": "Apache-2.0",
    "scripts": {
        "prepack": "node ../../scripts/verifyPackage.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/esfx/esfx.git"
    },
    "bugs": {
        "url": "https://github.com/esfx/esfx/issues"
    },
    "dependencies": {
        "@esfx/async-iter-fromsync": "^1.0.0-pre.39",
        "@esfx/async-iter-ordered": "^1.0.0-pre.39",
        "@esfx/async-iter-ordered-fromsync": "^1.0.0-pre.39",
        "@esfx/equatable": "^1.0.0-pre.39",
        "@esfx/internal-assert": "^1.0.0-pre.39",
        "@esfx/internal-guards": "^1.0.0-pre.39",
        "@esfx/iter-hierarchy": "^1.0.0-pre.39",
        "@esfx/iter-ordered": "^1.0.0-pre.39",
        "tslib": "^2.4.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
