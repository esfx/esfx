    {
        "runtime": "node",
        "target_platform": ["linux"],
        "target_arch": ["armhf"],
        "target": null,
        "supported_versions": ["^16", "^18"],
        "env": { "npm_config_arch": "arm" }
    },
    {
        "runtime": "node",
        "target_platform": ["alpine"],
        "target_arch": ["x64"],
        "target": null,
        "supported_versions": ["^16", "^18"]
    },
    {
        "runtime": "node",
        "target_platform": ["darwin"],
        "target_arch": ["x64", "arm64"],
        "target": null,
        "supported_versions": ["^16", "^18"]
    },

    {
        "runtime": "electron",
        "target": [23, 22, 21, 20, 19, 18, 17, 16, 15],
        "target_platform": ["win32"],
        "target_arch": ["ia32", "x64"],
        "args": ["--dist-url=https://electronjs.org/headers"]
    },
    {
        "runtime": "electron",
        "target": [23, 22, 21, 20, 19, 18, 17, 16, 15],
        "target_platform": ["linux"],
        "target_arch": ["x64", "arm64"],
        "args": ["--dist-url=https://electronjs.org/headers"]
