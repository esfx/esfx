    {
        "runtime": "electron",
        "target_platform": ["linux"],
        "target_arch": ["armhf"],
        "target": [23, 22, 21, 20, 19, 18, 17, 16, 15],
        "args": ["--dist-url=https://electronjs.org/headers"],
        "env": { "npm_config_arch": "arm" }
    },
    {
        "runtime": "electron",
        "target_platform": ["alpine"],
        "target_arch": ["x64"],
        "target": [23, 22, 21, 20, 19, 18, 17, 16, 15],
        "args": ["--dist-url=https://electronjs.org/headers"]
    },
    {
        "runtime": "electron",
        "target_platform": ["darwin"],
        "target_arch": ["x64", "arm64"],
        "target": [23, 22, 21, 20, 19, 18, 17, 16, 15],
        "args": ["--dist-url=https://electronjs.org/headers"]
